$res = "";

if (isset ( $_POST ["submit"] )) {
	// GoodsクラスのgetGoods()を使う
	$res = getGoods ( $allGoods, "F001" );
} else {
	$res = "";
}
?>
<html>
<head>
</head>
<body>
	<form action="">
		<input type="submit" name="submit">
	</form>
	<?php print $res; ?>
</body>
</html>